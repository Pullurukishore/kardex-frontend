# Production Environment Variables for Frontend
# Copy this to .env.production and update with your actual values

# API Configuration
NEXT_PUBLIC_API_URL=https://your-backend-cloud-run-url.run.app
NEXT_PUBLIC_API_BASE_URL=https://your-backend-cloud-run-url.run.app/api

# App Configuration
NEXT_PUBLIC_APP_NAME=KardexCare
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_ENVIRONMENT=production

# Authentication
NEXTAUTH_URL=https://your-frontend-domain.com
NEXTAUTH_SECRET=your-nextauth-secret-key-here

# Feature Flags
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_MONITORING=true

# External Services
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your-google-maps-api-key
NEXT_PUBLIC_LOCATIONIQ_KEY=your-locationiq-api-key

# Build Configuration
NEXT_TELEMETRY_DISABLED=1
NODE_ENV=production
